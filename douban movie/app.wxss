page {
  height: 100%;
}
.container {
  width: 100%;
  min-height: 100%;
  display: flex;
  flex-direction: column;
}
.page-session {
  background: #fff;
  border-width: 1px 0px;
  border-style: solid;
  border-color: #e6e6e6;
}
.page-session-spacing {
  margin-bottom: 30rpx;
  padding-bottom: 30rpx;
}
.session-search {
  padding: 20rpx 30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  background: #fff;
  border-width: 1px 0px;
  border-style: solid;
  border-color: #e6e6e6;
}
.session-search-spacing {
  margin-bottom: 30rpx;
}
.session-header {
  padding: 20rpx 30rpx;
  display: flex;
  align-items: center;
  /*justify-content: space-between;*/
}
.session-header-title {
  color:#4a4a4a;
  font-size: 36rpx;
  line-height: 1.42857;
  font-weight: 500;
  flex:1;
}
.session-header-more {
 font-size: 28rpx;
 color:#32cd32;
 font-weight: 500;
 position:relative;
 padding-right: 20rpx;
}
  /*伪元素，和元素一样可以设置样式，占据页面空间，但是没有标签，经常用于做>...用于做扫尾工作
  用标签做，有点杀鸡用牛刀，比如清除浮动*/
  .session-header-more:after{
    content: '';
    /*行内块级元素既能不把兄弟挤下去，inline-block不能设置大小，设置了也没用，block会挤下去*/
    display:inline-block;
    width:12rpx;
    height: 12rpx;
    border: 2rpx #32cd32 solid;
    border-bottom: none;
    border-left: none;
    transform: rotate(45deg);
    margin-top: -8rpx;
    position: absolute;
    top:50%;
  }
